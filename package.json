{
  "name": "anime-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy": "gh-pages -d build",
    "lint": "eslint . --ext ts,tsx --max-warnings 0 --fix",
    "preview": "vite preview",
    "format": "prettier . --ignore-path ./gitignore --write",
    "fix-style": "npm run lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.2",
    "@mui/material": "^5.15.2",
    "@mui/styled-engine-sc": "^6.0.0-alpha.10",
    "@reduxjs/toolkit": "^2.0.1",
    "@vitejs/plugin-react": "^4.2.1",
    "axios": "^1.6.7",
    "classnames": "^2.5.0",
    "dotenv": "^16.4.5",
    "gh-pages": "^6.1.1",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.44",
    "node-sass": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-horizontal-scrolling-menu": "^4.1.1",
    "react-intersection-observer": "^9.5.3",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "sass": "^1.69.6",
    "styled-components": "^6.1.6",
    "swiper": "^11.0.5",
    "vite": "^5.0.8"
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react": "^0.0.11",
    "@mate-academy/eslint-config-react-typescript": "^1.0.13",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "3.1.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.ts": "npm run fix-style",
    "*.tsx": "npm run fix-style"
  }
}
